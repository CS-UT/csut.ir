{
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/(fa|en)/?",
      "status": 301,
      "headers": {
        "Location": "/"
      }
    },
    {
      "src": "/assets/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/assets/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    }
  ]
}
