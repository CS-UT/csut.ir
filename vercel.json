{
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/",
      "has": [
        {
          "type": "header",
          "key": "x-vercel-ip-country",
          "value": "IR"
        }
      ],
      "status": 307,
      "headers": {
        "Location": "/fa"
      }
    },
    {
      "src": "/",
      "status": 307,
      "headers": {
        "Location": "/en"
      }
    },
    {
      "src": "/assets/fonts/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/assets/images/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(fa|en)/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=3600, s-maxage=3600"
      }
    }
  ]
}
